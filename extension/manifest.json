{
  "manifest_version": 2,

  "name": "Chaddon",
  "description": "This extension allows us to connect with global users.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Chaddon"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
